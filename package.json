    "@vitejs/plugin-react-swc": "^4.2.2",
    "concurrently": "^9.2.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "volta": {
    "node": "24.11.1"
